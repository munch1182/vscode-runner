{
  "name": "munch1182-vscode-runner",
  "displayName": "runner by munch1182",
  "description": "runner",
  "publisher": "munch1182",
  "author": "munch1182",
  "license": "MIT",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "munch1182-vscode-runner.runFile",
        "title": "Run File"
      },
      {
        "command": "munch1182-vscode-runner.runProject",
        "title": "Run Project"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "munch1182-vscode-runner.runFile",
          "group": "navigation"
        },
        {
          "command": "munch1182-vscode-runner.runProject",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "vscode-runner",
      "properties": {
        "vscode-runner.fileRunner": {
          "type": "object",
          "default": {
            ".js": "cd $dir && node $filename",
            ".ts": "cd $dir && ts-node $filename",
            ".rs": "cd $dir && rustc $filename && $dir$sep$filenameWithoutExt && del $filenameWithoutExt.exe && del $filenameWithoutExt.pdb",
            ".py": "cd $dir && python -u $filename",
            ".ps1": "cd $dir && .$sep$filename",
            ".bat": "cd $dir && .$sep$filenameWithoutExt"
          }
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/munch1182/vscode-runner.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile &&&& npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
